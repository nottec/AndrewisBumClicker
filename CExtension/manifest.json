{
  "name": "Clicker",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "128": "images/clicker-128.png"
  },
  "content_scripts": [
      {
      "js": ["scripts/content.js"],
      "matches": ["<all_urls>"]
      }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "abumwebex.html"
  },
  "permissions":["storage"]
}











